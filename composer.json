        {
            "name": "hackout",
            "email": "hackout@vip.qq.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=11.0",
        "simplecms/framework": ">=1.0",
        "laravel/sanctum": ">=4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCMS\\Company\\CompanyServiceProvider"
            ],
            "aliases": {
                "CompanyAuthenticatable": "SimpleCMS\\Company\\Facades\\CompanyAuthenticatable"
            }
        }
    }
